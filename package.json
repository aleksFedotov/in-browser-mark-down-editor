		"svelte-fragment-component": "^1.2.0",
		"svelte-htm": "^1.2.0",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.7",
		"vitest-svelte-kit": "^0.0.7"
	},
	"type": "module",
	"postinstall": "patch-package",
	"dependencies": {
		"date-fns": "^2.29.3",
